config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/18000000.wmac'
	option band '2g'
	option htmode 'HT40'
	option channel '6'
	option legacy_rates '1'
	option txpower '20'
	option cell_density '0'
	option country 'PT'

config wifi-iface 'radio0_acjl'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ACJL-NETWORK'
	option encryption 'sae-mixed'
	option key '{{ wireless_acjl_password }}'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'

config wifi-iface 'radio0_acjl_legacy'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ACJL-LEGACY'
	option encryption 'psk-mixed'
	option key '{{ wireless_acjl_password }}'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option isolate '1'

config wifi-iface 'radio0_acjl_guest'
	option device 'radio0'
	option network 'guest'
	option mode 'ap'
	option ssid 'ACJL-GUEST'
	option encryption 'sae-mixed'
	option key '{{ wireless_acjl_guest_password }}'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option isolate '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path '1a143000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option band '5g'
	option cell_density '0'
	option country 'PT'
	option htmode 'HE160'
	option txpower '26'
	option channel '100'

config wifi-iface 'radio1_acjl'
	option device 'radio1'
	option mode 'ap'
	option ssid 'ACJL-NETWORK'
	option encryption 'sae-mixed'
	option key '{{ wireless_acjl_password }}'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option network 'lan'
